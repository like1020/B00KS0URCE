{
  "name": "大文学无错小说网",
  "version": 100,
  "category": 1,
  "url": "https://www.dawenxue.net",
  "charset": "utf8",
  "metadata": {
    "name": [".s2>a", "#info>h1"],
    "author": [".s4", "#info>p:first-child"],
    "cover": ["#fmimg>img@attr->src"],
    "summary": ["#intro>p"],
    "category": [".s1@js->${this}.match('[(.*)]')[1]", ".con_top>a:nth-child(2)"],
    "status": ["td:nth-child(6)>a"],
    "update": ["li>span:nth-child(5)", "#info>p:nth-child(4)@js->${this}.match('最后更新：(.*)')[1]"],
    "lastChapter": [".s3>a","#info>p:nth-child(5)>a"],
    "link": [".s2>a@attr->href"],
    "catalog": []
  },
  "catalog": {
    "list": "#list > dl > dd",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "#content"
  },
  "search": {
    "link": "https://www.dawenxue.net/modules/article/search.php@post->searchkey=${key}&searchtype=keywords",
    "list": ".novelslist2>ul>li:gt(1)"
  }
}
