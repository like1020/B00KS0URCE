{
  "name": "疯情书库",
  "version": 100,
  "category": 1,
  "url": "https://m.aaread.club/",
  "charset": "utf-8",
  "metadata": {
    "name": [".book-cell h4"],
    "author": [".book-author"],
    "cover": ["img.book-cover@attr->src"],
    "summary": [".book-desc"],
    "category": [".tag-small:nth-child(1)"],
    "status": [".tag-small:nth-child(2)"],
    "update": ["meta[property=og:novel:update_time]@attr->content"],
    "lastChapter": ["meta[property=og:novel:latest_chapter_name]@attr->content"],
    "link": [".book-cell h4@attr->href"],
    "catalog": [".book-meta:nth-child(2)@attr->href"]
  },
  "catalog": {
    "list": "#volumes li",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "#chapterContent > section"
  },
  "search": {
    "link": "https://m.aaread.club/search?kw=${key}",
    "list": ".book-layout"
  }
}
