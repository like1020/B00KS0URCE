{
  "name": "疯情书库",
  "version": 100,
  "category": 1,
  "url": "https://aaread.club/",
  "charset": "",
  "metadata": {
    "name": [".book-mid-info > h4 > a"],
    "author": [".book-mid-info > .author > a:nth-child(2)"],
    "cover": ["a > img@attr->src"],
    "summary": ["a > p", ".u"],
    "category": [".tuijian > a.tag-bg-blue"],
    "status": [".book_info > .infos > span > span@js->${this}.match('状态(.*)')[1]"],
    "update": [".book_info > .infos > span > span@js->${this}.match('最后更新：(.*)状态')[1]"],
    "lastChapter": [".book_info > .upd > a"],
    "link": ["a@attr->href"],
    "catalog": []
  },
  "catalog": {
    "list": ".book_list > ul > li",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "#htmlContent"
  },
  "search": {
    "link": "https://aaread.club/search?kw=${key}",
    "list": ".book-img-text li"
  },
  "rank": {
    "link": [
      {
        "name": "排行榜",
        "link": "http://zuopinj.com/alltotal/"
      },
      {
        "name": "推荐榜",
        "link": "http://zuopinj.com/allisgood/"
      },
      {
        "name": "下载榜",
        "link": "http://zuopinj.com/alldown/"
      },
      {
        "name": "更新榜",
        "link": "http://zuopinj.com/newupdate/"
      }
    ],
    "list": ".book-img-text > ul > li",
    "page": {
      "index": 1,
      "limit": 30,
      "begin": "",
      "next": "index_${index}.html"
    }
  }
}
